<?xml version="1.0" encoding="utf-8"?>
<configuration>

  <configSections>
    <section name="ServiceRegister" type="DAL.Configuration.ServiceRegisterConfigSection, DAL"/>
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
  </startup>
  <appSettings>
    <add key="xmlfile" value="./userstorage.xml"/>
  </appSettings>

  <ServiceRegister>
    <Services>
      <add serviceType="Master" path="master"/>
      <add serviceType="Slave" path="slave1" ip="192.168.1.1" port="11000"/>
      <add serviceType="Slave" path="slave2" ip="192.168.1.2" port="11001"/>
      <add serviceType="Slave" path="slave3" ip="192.168.1.3" port="11002"/>
      <add serviceType="Slave" path="slave4" ip="192.168.1.4" port="11003"/>
    </Services>
  </ServiceRegister>


  <system.diagnostics>
    <switches>
      <add name="Data" value="1"/>
    </switches>
  </system.diagnostics>

</configuration>