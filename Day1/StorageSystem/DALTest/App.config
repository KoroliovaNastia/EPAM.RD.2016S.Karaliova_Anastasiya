<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <section name="StartupFolders" type="ConfigSectionTester.StartupFoldersConfigSection, ConfigSectionTester"/>

  <appSettings>
    <add key="xmlfile" value="./userstorage.xml" />
  </appSettings>
  <ServiceRegister>
    <Services>
      <add serviceType="Master" path="master" />
      <add serviceType="Slave" path="slave1" />
    </Services>
  </ServiceRegister>


  <system.diagnostics>
    <switches>
      <add name="Data" value="1" />
    </switches>
  </system.diagnostics>

</configuration>